[alias]
    co = checkout
    br = branch
    ff = merge --ff-only
    st = status
    ci = commit
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    fetchpr = !sh -c 'git fetch upstream pull/$1/head:$2' -
    pushf = push --force-with-lease
[user]
    name = Andy Gocke
    email = andy@commentout.net
[push]
    default = simple
[url "https://"]
    insteadOf = git://
[init]
    defaultBranch = main
[fetch]
    prune = true
